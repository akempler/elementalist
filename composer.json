{
    "name": "akempler/elementalist",
    "description": "Install Drupal 9 with Composer.",
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https:\/\/packages.drupal.org\/8"
        },
        {
            "type": "path",
            "url": "upstream-configuration"
        },
        {
            "type": "composer",
            "url": "https:\/\/asset-packagist.org"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/actions_permissions": "^4.1",
        "drupal/address": "^1.10",
        "drupal/admin_toolbar": "^3.1",
        "drupal/components": "^2.4",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/config_split": "^1.7",
        "drupal/core-composer-scaffold": "^9.2",
        "drupal/core-recommended": "^9.2",
        "drupal/devel": "^4.1",
        "drupal/entity": "^1.3",
        "drupal/extlink": "^1.6",
        "drupal/facets": "^2.0",
        "drupal/field_config_cardinality": "^1.1",
        "drupal/field_group": "^3.2",
        "drupal/geocoder": "^3.25",
        "drupal/geofield": "^1.38",
        "drupal/google_tag": "^1.4",
        "drupal/honeypot": "^2.0",
        "drupal/layout_builder_modal": "^1.1",
        "drupal/layout_builder_restrictions": "^2.12",
        "drupal/migrate_plus": "^5.2",
        "drupal/migrate_tools": "^5.1",
        "drupal/multiselect": "^1.2",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.9",
        "drupal/redirect": "^1.7",
        "drupal/search_api": "^1.23",
        "drupal/search_api_solr": "^4.2",
        "drupal/select2": "^1.13",
        "drupal/slick": "^2.5",
        "drupal/smart_trim": "^1.3",
        "drupal/stage_file_proxy": "^1.2",
        "drupal/token": "^1.10",
        "drupal/views_bulk_operations": "^4.1",
        "drupal/webform": "^6.1",
        "drush-ops/behat-drush-endpoint": "^9.4",
        "drush/drush": "^10",
        "npm-asset/select2": "^4.0",
        "oomphinc/composer-installers-extender": "^2.0"
    },
    "require-dev": {
        "behat/behat": "^3.10",
        "behat/mink": "^1.9",
        "behat/mink-extension": "^2.3",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
        "dmore/behat-chrome-extension": "^1.3",
        "drupal/coder": "^8.3",
        "drupal/core-dev": "^9.2",
        "drupal/drupal-driver": "^2.1",
        "drupal/drupal-extension": "^4.1",
        "drupal/entity_browser": "^2.6",
        "genesis/behat-fail-aid": "^3.7",
        "jcalderonzumba/mink-phantomjs-driver": "^0.3.3",
        "mikey179/vfsstream": "^1.6",
        "phpspec/prophecy-phpunit": "^2",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "conflict": {
        "drupal\/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "patches": {
            "drupal/paragraphs": {
                "Integrity constraint violation: 1048 Column 'langcode' cannot be null": "https://www.drupal.org/files/issues/2022-02-28/paragraphs-2901390-103-withtests.patch"
            },
            "drupal/core": {
                "CKEditor table properties not clickable due to interaction with jQuery UI dialogs": "https://www.drupal.org/files/issues/2022-02-28/3065095-40.ckeditor-table-prop.patch"
            }
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": ".\/web"
            },
            "allowed-packages": [
                "pantheon-systems\/drupal-integrations"
            ],
            "file-mapping": {
                "[project-root]\/.editorconfig": false,
                "[project-root]\/pantheon.upstream.yml": false,
                "[project-root]\/.gitattributes": false,
                "[web-root]/sites/development.services.yml": false
            }
        },
        "installer-paths": {
            "web\/core": [
                "type:drupal-core"
            ],
            "web\/libraries\/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web\/modules\/contrib\/{$name}": [
                "type:drupal-module"
            ],
            "web\/profiles\/contrib\/{$name}": [
                "type:drupal-profile"
            ],
            "web\/themes\/contrib\/{$name}": [
                "type:drupal-theme"
            ],
            "drush\/Commands\/contrib\/{$name}": [
                "type:drupal-drush"
            ],
            "web\/modules\/custom\/{$name}": [
                "type:drupal-custom-module"
            ],
            "web\/profiles\/custom\/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web\/themes\/custom\/{$name}": [
                "type:drupal-custom-theme"
            ],
            "web\/private\/scripts\/quicksilver\/{$name}\/": [
                "type:quicksilver-script"
            ]
        },
        "installer-types": ["bower-asset", "npm-asset"],
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal\/core": "-p2"
        },
        "build-env": {
            "export-configuration": "drush config-export --yes"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "scripts": {
        "lint": "find web\/modules\/custom web\/themes\/custom -name '*.php' -exec php -l {} \\;",
        "code-sniff": [
            ".\/vendor\/bin\/phpcs --standard=Drupal --extensions=php,module,inc,install,test,profile,theme,css,info,txt,md --ignore=node_modules,bower_components,vendor .\/web\/modules\/custom",
            ".\/vendor\/bin\/phpcs --standard=Drupal --extensions=php,module,inc,install,test,profile,theme,css,info,txt,md --ignore=node_modules,bower_components,vendor,dist .\/web\/themes\/custom",
            ".\/vendor\/bin\/phpcs --standard=Drupal --extensions=php,module,inc,install,test,profile,theme,css,info,txt,md --ignore=node_modules,bower_components,vendor .\/web\/modules\/custom",
            ".\/vendor\/bin\/phpcs --standard=Drupal --extensions=php,module,inc,install,test,profile,theme,css,info,txt,md --ignore=node_modules,bower_components,vendor,dist .\/web\/themes\/custom"
        ]
    }
}
